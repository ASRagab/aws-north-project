{
  "name": "msk-lambda-producer",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build:ts": "esbuild src/index.ts --bundle --minify --sourcemap --platform=node --target=es2020 --outfile=dist/index.js",
    "build:deps": "cp package.json package-lock.json dist && npm ci --omit=dev --prefix dist --quiet",
    "build": "npm run clean && npm run build:ts && npm run build:deps",
    "package:local": "cd dist && zip -qr ../../msk-lambda-producer.zip .",
    "package:remote": "cd dist && zip -qr ../../msk-lambda-producer.zip .",
    "deploy:local": "npm run build && npm run package:local",
    "deploy:remote": "npm run build && npm run package:remote"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/aws-lambda": "^8.10.145",
    "@types/node": "^22.7.5",
    "esbuild": "^0.24.0",
    "rimraf": "^6.0.1",
    "typescript": "^5.6.2"
  },
  "dependencies": {
    "aws-msk-iam-sasl-signer-js": "github:aws/aws-msk-iam-sasl-signer-js",
    "aws-sdk": "^2.1691.0",
    "dayjs": "^1.11.10",
    "kafkajs": "^2.2.4"
  }
}
